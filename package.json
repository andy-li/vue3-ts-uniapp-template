{
  "name": "uni-preset-vue",
  "version": "0.0.0",
  "scripts": {
    "dev:app": "uni -p app",
    "dev:h5": "uni",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:h5": "uni build",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build-qa1:h5": "uni build --mode qa1",
    "build-qa2:h5": "uni build --mode qa2",
    "build-qa3:h5": "uni build --mode qa3",
    "build-qa4:h5": "uni build --mode qa4",
    "build-release:h5": "uni build --mode release",
    "build-qa1:app": "uni build -p app --mode qa1",
    "build-qa2:app": "uni build -p app --mode qa2",
    "build-qa3:app": "uni build -p app --mode qa3",
    "build-qa4:app": "uni build -p app --mode qa4",
    "build-release:app": "uni build -p app --mode release",
    "build-prod:app": "uni build -p app --mode production",
    "build-qa1:mp-weixin": "uni build -p mp-weixin --mode qa1",
    "build-qa2:mp-weixin": "uni build -p mp-weixin --mode qa2",
    "build-qa3:mp-weixin": "uni build -p mp-weixin --mode qa3",
    "build-qa4:mp-weixin": "uni build -p mp-weixin --mode qa4",
    "build-release:mp-weixin": "uni build -p mp-weixin --mode release",
    "build-prod:mp-weixin": "uni build -p mp-weixin --mode production",
    "prepare": "husky install",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "commit": "git-cz"
  },
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-app-plus": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-components": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-h5": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-alipay": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-baidu": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-kuaishou": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-lark": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-qq": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-toutiao": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-mp-weixin": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-quickapp-webview": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-ui": "^1.4.12",
    "@vitejs/plugin-vue": "^2.2.4",
    "axios": "^0.26.1",
    "axios-adapter-uniapp": "^0.1.3",
    "axios-retry": "^3.2.4",
    "pinia": "^2.0.12",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@dcloudio/types": "^2.5.18",
    "@dcloudio/uni-automator": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/uni-cli-shared": "^3.0.0-alpha-3040120220308001",
    "@dcloudio/vite-plugin-uni": "^3.0.0-alpha-3040120220308001",
    "@rushstack/eslint-patch": "^1.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.1",
    "sass": "^1.49.9",
    "typescript": "^4.6.3",
    "vite": "^2.8.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
